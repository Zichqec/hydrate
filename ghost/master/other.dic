OnTranslate
{
	_talk = reference0
	
	//Process embedded elements in script input
	if reference1 == "" && reference2 == ""
	{
		_talk = EVAL('"' + REPLACE(_talk,'"','""') + '"')
	}
	
	_talk = REPLACE(_talk,"\b[4]","\b[4,--fallback=2]")
	
	TOSTR(_talk)
}

OnAnchorSelect
{
	if "http://" _in_ reference0 || "https://" _in_ reference0; "\j[""%(reference0)""]"
}